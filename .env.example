# OpenExpoOTA Client Configuration

# API connection settings
OTA_API_URL=http://localhost:3000/api

# App identifier - this is the app slug from your backend
OTA_APP_SLUG=your-app-slug

# App access key - this is generated in the backend and provided by the CLI
OTA_APP_KEY=your-app-key

# Update check interval (in milliseconds)
OTA_CHECK_INTERVAL=60000

# Default release channel
OTA_CHANNEL=development

# Update behavior - 'auto' or 'manual'
OTA_UPDATE_MODE=auto

